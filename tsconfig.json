{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": "./src",
		"paths": {
			"@/interactive/*": ["components/interactive/*"],
			"@/ui/*": ["components/ui/*"],
			"@/sections/*": ["components/sections/*"],
			"@/styles/*": ["styles/*"],
			"@/utils/*": ["utils/*"],
			"@layouts/*": ["layouts/*"],
			"@config/*": ["config/*"],
			"@/types/*": ["types/*"]
		}
	}
}
